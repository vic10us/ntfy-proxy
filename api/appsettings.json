{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Ntfy": {
    "ServerUrl": "http://localhost:5000",
    "Notifications": {
      "media.play": [ "/media_play" ],
      "media.pause": [ "/media_pause" ],
      "media.stop": [ "/media_stop" ],
      "media.resume": [ "/media_resume" ],
      "media.scrobble": [ "/media_scrobble" ],
      "media.rate": [ "/media_rate" ],
      "playback.started": [ "/playback_started" ],
      "device.new": [ "/device_new" ],
      "library.new": [ "/library_new" ],
      "library.on.deck": [ "/library_on_deck" ],
      "admin.database.backup": [ "/admin_database_backup" ],
      "admin.database.corrupt": [ "/admin_database_corrupt" ]
    }
  }
}
